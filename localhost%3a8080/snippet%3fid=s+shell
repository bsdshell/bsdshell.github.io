    sed 's/.*\(Name=\"[^"]*\"\)\s\(MyDir=\"[^"]*\"\).*/\1 \2/g' f1.txt > f2.txt //Match Name=xx, MyDir=xx 2 col
    sed -i -e 's/oldstring/newstring/g' *  // replace oldstr to newstr, replace all, sed replace
    sed '/line3/,/line9/d' oldfile > newfile  // delete lines from line contains line3 to line contains line9
    sed -E '/grep|vim/d'  // posix regular extension -E
    gsed -i_backup 's/oldstr/newstr/' *.html  // file is backed up with *.html_backup
    sed -E 's/ *//g'  # remove all whitespace, sed -E 's/\s*//g' does not work
    --
    -- remove whitespaces from each filename in all files in current directory.
    for i in *; do var=$(echo $i | sed -E 's/ *//g'); mv "$PWD/$i" "$PWD/$var"; done
